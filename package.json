{
  "name": "@wearethreebears/barebones",
  "version": "0.4.1",
  "bin": {
    "theme": "bin/index.js"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "component:build": "node ./generator/scaffold.js build",
    "component:destroy": "node ./generator/scaffold.js destroy",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/vue3": "^6.4.22",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitest/ui": "^0.15.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.0",
    "happy-dom": "^5.3.1",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.2.0",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.7"
  },
  "description": "This template should help get you started developing with Vue 3 in Vite.",
  "main": "index.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wearethreebears/base-components.git"
  },
  "author": "Liam Hall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wearethreebears/base-components/issues"
  },
  "homepage": "https://github.com/wearethreebears/base-components#readme"
}
